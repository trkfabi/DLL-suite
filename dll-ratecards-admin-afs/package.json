{
	"name": "dll-rcm",
	"version": "2.35.0",
	"scripts": {
		"ng": "ng",
		"format": "./node_modules/.bin/prettier --loglevel warn --write \"{{package,tslint,tsconfig,.angular.cli}.json,*.js,src/**/*.{ts,scss,json,html}}\"",
		"lint": "./node_modules/.bin/tslint -p tsconfig.json -c tslint.json",
		"prestart": "npm run format && npm run lint -- --format stylish",
		"start": "ng serve --port=4200 -o --aot",
		"start-dev": "npm run start -- --configuration=dev",
		"start-acc": "npm run start -- --configuration=acc",
		"server-dev": "node server/server.js",
		"prebuild": "mkdir -p reports && npm run lint -- -t json -o \"reports/tslint.json\"",
		"build": "ng build",
		"test": "ng test",
		"pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
